<Alloy>
	<Window class="container">
		<View id="menuView">
			<Label id="title"></Label>
			<Button id="startButton"></Button>
		</View>
		<View id="gameView">
			<Button id="pauseButton"></Button>
			<Label id="scoreLabel" class="label"></Label>
			<Label id="levelUpLabel" class="label"></Label>
			<View id="background"></View>
			<View id="grid"></View>
			<View id="leftStump"></View>
			<View id="middleStump"></View>
			<View id="rightStump"></View>
			<View id="soil"></View>
			<Button id="leftChopButton" class="chopButton"></Button>
			<Button id="rightChopButton" class="chopButton"></Button>
			<View id="player"></View>
		</View>
	</Window>
</Alloy>